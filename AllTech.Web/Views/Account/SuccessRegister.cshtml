@model User
@{
    ViewData["Title"] = "پایان ثبت نام";
}

    <div class="alert alert-success">
        <h2>
            @Model.UserName عزیز!
        </h2>
        <p>
            ثبت نام شما انجام شد. یک ایمیل حاوی لینک فعالسازی به آدرس @Model.Email ارسال شد.
        </p>
        <p>
            لطفا جهت فعاسازی وارد ایمیل خود شده و روی لینک ارسال شده کلیک کنید.
        </p>
    </div>
    

